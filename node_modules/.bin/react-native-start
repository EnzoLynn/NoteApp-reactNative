#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/bash" ]; then
  "$basedir/bash"  "$basedir/../react-native/packager/packager.sh" "$@"
  ret=$?
else 
  bash  "$basedir/../react-native/packager/packager.sh" "$@"
  ret=$?
fi
exit $ret
